{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes": "off",
        "require-jsdoc": "off",
        "prefer-const": "off",
        "eol-last": "off",
        "indent": "off",
        "space-before-blocks": "off",
        "keyword-spacing": "off",
        "max-len": "off",
        "valid-jsdoc": "off",
        "brace-style": "off",
        "no-unused-vars": "off"
    }
}
